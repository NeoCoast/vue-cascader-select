    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  },
  "gitHooks": {
    "pre-commit": "npm run test:unit && lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
