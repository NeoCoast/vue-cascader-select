    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "codecov": "^3.6.5",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.5.0",
    "minimist": "^1.2.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.0.0-rc.1"
  },
